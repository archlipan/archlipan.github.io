---
layout: null
---
<?xml version="1.0"?>
<rss version="2.0"
    xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <title>archlipan</title>
        <link>archlipan.github.io</link>
        <atom:link href="archlipan.github.io/rss.xml" rel="self" type="application/rss+xml" />
        <description>Lee</description>
        <language>zh-cn</language>
        <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts limit:10 %}
        <item>
            <title>{{ post.title }}</title>
            <link>archlipan.github.io{{ post.url }}</link>
            <pubDate>{{ post.date | date_to_string }}</pubDate>
            <author>Lee</author>
            <guid>archlipan.github.io{{ post.id }}</guid>
            <description>{{ post.content | xml_escape }}</description>
        </item>
    {% endfor %}
    </channel>
</rss>